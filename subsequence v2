using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSlight
{
    class Program
    {
        static void Main(string[] args)
        {
            int step = 7;
            int border = 96;
            int startingNumber = 5;

            for (int i = 5 ; startingNumber <= border; startingNumber += step)
            {
                Console.WriteLine(startingNumber);
            }

            Console.ReadKey();
        }
    }
}
